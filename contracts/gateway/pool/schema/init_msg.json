{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "open_claim",
    "open_deposit",
    "open_withdraw",
    "period",
    "reward_rate",
    "reward_token",
    "share_token",
    "start_time"
  ],
  "properties": {
    "open_claim": {
      "type": "boolean"
    },
    "open_deposit": {
      "type": "boolean"
    },
    "open_withdraw": {
      "type": "boolean"
    },
    "period": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "reward_rate": {
      "$ref": "#/definitions/Decimal256"
    },
    "reward_token": {
      "$ref": "#/definitions/HumanAddr"
    },
    "share_token": {
      "$ref": "#/definitions/HumanAddr"
    },
    "start_time": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "definitions": {
    "Decimal256": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal256(1_000_000_000_000_000_000) == 1.0 The greatest possible value that can be represented is 115792089237316195423570985008687907853269984665640564039457.584007913129639935 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "HumanAddr": {
      "type": "string"
    }
  }
}
